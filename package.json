{
  "name": "cdk8s",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "for file in apps/*/*.js; do npx node \"${file}\"; done",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next",
    "lint": "npx eslint \"**/*.ts\"",
    "fmt": "npx prettier --check --write \"**/*.ts\"",
    "clean": "rm -rf dist/",
    "fullbuild": "yarn run clean && yarn run fmt && yarn run lint && yarn run compile && yarn run synth",
    "build": "yarn run clean && yarn run compile && yarn run synth"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cdk8s": "^2.7.108",
    "cdk8s-plus-26": "^2.4.90",
    "constructs": "^10.2.69",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "cdk8s-cli": "^2.2.110",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "yarn": "^1.22.19"
  }
}
