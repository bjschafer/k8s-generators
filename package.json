{
  "name": "cdk8s",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "for file in apps/*/*.js; do npx node \"${file}\"; done",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next",
    "lint": "npx eslint \"**/*.ts\"",
    "fmt": "npx prettier --check --write \"**/*.ts\"",
    "clean": "rm -rf dist/",
    "fullbuild": "yarn run clean && yarn run fmt && yarn run lint && yarn run compile && yarn run synth",
    "build": "yarn run clean && yarn run compile && yarn run synth"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cdk8s": "^2.68.2",
    "cdk8s-plus-27": "^2.7.55",
    "constructs": "^10.3.0",
    "eslint": "^8.53.0",
    "js-yaml": "4.1.0",
    "prettier": "^3.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.0",
    "cdk8s-cli": "^2.170.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "yarn": "^1.22.19"
  }
}
