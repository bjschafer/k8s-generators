{
  "name": "cdk8s",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "for file in apps/*/*.js; do npx node \"${file}\"; done",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next",
    "lint": "npx eslint \"**/*.ts\"",
    "fmt": "npx prettier --check --write \"**/*.ts\"",
    "clean": "rm -rf dist/",
    "fullbuild": "yarn run clean && yarn run fmt && yarn run lint && yarn run compile && yarn run synth",
    "build": "yarn run clean && yarn run compile && yarn run synth"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cdk8s": "^2.7.36",
    "cdk8s-plus-25": "^2.6.1",
    "constructs": "^10.1.281",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "cdk8s-cli": "^2.1.161",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "yarn": "^1.22.19"
  }
}
